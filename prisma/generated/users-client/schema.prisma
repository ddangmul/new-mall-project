datasource db2 {
  provider = "sqlite"
  url      = env("DATABASE_URL_2")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/users-client"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
